{
  "name": "configcat-vue",
  "description": "A community-maintained SDK for integrating ConfigCat feature flags into Vue.js applications.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/configcat-vue.umd.cjs",
  "module": "./dist/configcat-vue.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/configcat-vue.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/configcat-vue.umd.cjs"
      }
    }
  },
  "version": "2.6.1",
  "keywords": [
    "configcat",
    "ConfigCat",
    "configcat vue",
    "configcat vue3",
    "feature-flags",
    "feature flags",
    "feature-toggle",
    "feature toggle",
    "feature-management",
    "feature management",
    "feature-flag",
    "feature flag",
    "feature flags vue",
    "vue3 feature flags",
    "vite-plugin-vue-configcat"
  ],
  "author": "Chavez Harris",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.10",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.4",
    "vue-tsc": "^3.1.2"
  },
  "dependencies": {
    "@configcat/sdk": "^1.0.2"
  }
}
